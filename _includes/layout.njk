                        <div class="flex-1 mb-6">
                            <label class="block text-zinc-900 leading-none text-sm font-semibold mb-2" for="ratio-width">
                            Ratio Width
                            </label>
                            <input class="text-sm appearance-none border rounded-xl w-full py-2 px-3 h-10 text-zinc-700 leading-none  bg-zinc-50 focus:bg-white border-zinc-300 focus:outline-indigo-600 focus:shadow-outline hover:border-zinc-500" type="number" id="ratio-width" name="ratio-width" value="{{ratioWidth}}">
                        </div>
                        <div class="flex-1 mb-4">
                            <label class="block text-zinc-900 leading-none text-sm font-semibold mb-2" for="ratio-height">
                            Ratio Height
                            </label>
                            <input class="text-sm appearance-none border rounded-xl w-full py-2 px-3 h-10 text-zinc-700 leading-none  bg-zinc-50 focus:bg-white border-zinc-300 focus:outline-indigo-600 focus:shadow-outline hover:border-zinc-500" type="number" id="ratio-height" name="ratio-height" value="{{ratioHeight}}">
